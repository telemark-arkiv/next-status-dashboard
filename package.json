{
  "name": "next-status-dashboard",
  "description": "Dashboard for statuses",
  "version": "1.1.3",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "now-deploy": "npm test && now --npm --alias"
  },
  "keywords": [
    "dashboard",
    "next",
    "status",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/next-status-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/next-status-dashboard/issues"
  },
  "now": {
    "aliases": [
      "dashboard.status.t-fk.win"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "coveralls": "2.11.15",
    "nsp": "2.6.2",
    "standard": "8.6.0",
    "tap": "10.0.0"
  },
  "dependencies": {
    "isomorphic-fetch": "2.2.1",
    "muicss": "0.9.9-rc2",
    "next": "1.2.3"
  }
}
